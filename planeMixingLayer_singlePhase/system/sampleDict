FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    L21
    {
        type    uniform;
        axis    distance;
        start   (0 0.21 0);
        end     (0.4 0.21 0);
        nPoints 100;
    }
);

fields
(
    alpha.air
);



