FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1";
    object      gradp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 1 0);

boundaryField
{
    inlet
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            extrapolatedCalculated;
        value           uniform (0 1 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


